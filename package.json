{
  "name": "westures-example",
  "version": "1.0.0",
  "description": "Simple static example page of the Westures library in action",
  "author": "Michael van der Kamp <michael.vanderkamp@usask.ca>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvanderkamp/westures-example.git"
  },
  "homepage": "https://github.com/mvanderkamp/westures-example#readme",
  "bugs": {
    "url": "https://github.com/mvanderkamp/westures-example/issues"
  },
  "keywords": [
    "westures",
    "example",
    "gesture",
    "library",
    "multitouch"
  ],
  "scripts": {
    "build": "parcel build",
    "build:debug": "parcel build --no-optimize",
    "start": "http-server ."
  },
  "dependencies": {
    "westures": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^7.8.1",
    "http-server": "^14.1.1",
    "parcel": "^2.8.3"
  },
  "targets": {
    "default": {
      "context": "browser",
      "source": "sample.js",
      "distDir": "dist",
      "includeNodeModules": true,
      "isLibrary": false,
      "outputFormat": "global",
      "publicUrl": "./",
      "sourceMap": true
    }
  }
}
